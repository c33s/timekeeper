{
    "name": "dantleech/timekeeper",
    "description": "Keeps Time",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0",
        "phpactor/console-extension": "^0.1.0@dev",
        "hoa/compiler": "^3.0@dev",
        "phpactor/text-document": "^1.0@dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\Timekeeper\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\Timekeeper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\Timekeeper\\TimekeeperExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
